{
  "name": "vidstreaming-anime-downloader",
  "version": "1.3.0",
  "description": "Api for downloading or watching anime videos from gogo-stream.com/vidstreaming.io",
  "main": "dist/vidstreaming.js",
  "scripts": {
    "start": "nodemon src/index.js",
    "transpile": "babel ./src --out-dir ./dist --no-comments",
    "clean": "rimraf ./dist",
    "build": "npm-run-all clean transpile",
    "watch": "babel ./src --out-dir ./dist --no-comments -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sb-gravity100/vidstreaming-api-test.git"
  },
  "keywords": [
    "anime",
    "downloader",
    "vidstreaming",
    "gogostream",
    "anime-downloader",
    "cli"
  ],
  "author": "SB",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sb-gravity100/vidstreaming-api-test/issues"
  },
  "homepage": "https://github.com/sb-gravity100/vidstreaming-api-test#readme",
  "dependencies": {
    "async": "^3.2.0",
    "axios": "^0.21.1",
    "axios-retry": "^3.1.9",
    "boxen": "^5.0.0",
    "chalk": "^4.1.0",
    "cheerio": "^1.0.0-rc.5",
    "clui": "^0.3.6",
    "lodash": "^4.17.21",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.14",
    "@babel/node": "^7.13.13",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@babel/runtime": "^7.13.10",
    "eslint": "^7.23.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "2.22.1",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  },
  "nodemonConfig": {
    "execMap": {
      "js": "babel-node --harmony --trace-warnings"
    },
    "ignore": [
      "dist/"
    ]
  },
  "bin": {
    "vidstreaming": "dist/bin/index.js"
  }
}
