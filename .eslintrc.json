{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "no-undef": "off",
        "no-undefined": "off",
        "no-unused-vars": "off",
        "indent": "off",
        "no-unused-expressions": "off"
    }
}
