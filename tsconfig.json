{
    "exclude": ["node_modules/", "./index.js", "./index.d.js"],
    "include": ["src/vidstreaming.ts"],
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "lib": [],
        "declaration": true,
        "emitDeclarationOnly": true,
        "sourceMap": true,
        "outFile": "./index.js",
        "strict": false,
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    }
}
